generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id     Int     @id @default(autoincrement())
  name   String?
  userId Int
}

model User {
  name          String?
  picture       String?
  email         String  @unique
  id            Int     @id @default(autoincrement())
  provider      String?
  access_token  String? @db.Text
  refresh_token String? @db.Text
}

model Suscriber {
  name  String?
  email String
  id    Int     @id @default(autoincrement())
}
